{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CorsHosts": "*",
  "UseHttps": true,
  "Port": 5000,
  "EmailOptions": {
    "Sender": "Reniwn",
    "Subject": "Reniwn Account Created"
  },
  "GeneratedPasswordOptions": {
    "Length": 8,
    "HasLowerCaseAlpha": true,
    "HasUpperCaseAlpha": true,
    "HasNumbers": true,
    "HasSpecialCharacters": false
  },
  "API": {
    "BasePath": "https://www.myreniwn.com",
    "Label": "reniwn",
    "Token": "[API_TOKEN]"
  },
  "MailerSend": {
    "BasePath": "https://api.mailersend.com/v1",
    "Token":"[API_TOKEN]",
    "Email": "[MAIL]@reniwn.com",
    "Password": "[PASSWORD]",
    "TemplateName": "Account Created"
  }
}